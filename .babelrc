{
  "presets": ["module:@react-native/babel-preset"],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "@api": "./src/apis",
        "@services": "./src/services/",
        "@shared": "./src/shared/",
        "@assets": "./src/assets/",
        "@constants": "./src/constants/",
        "@hooks": "./src/hooks/",
        "@navigation": "./src/navigation",
        "@components": "./src/components",
        "@store": "./src/store",
        "@utils": "./src/utils",
        "@interfaces": "./src/interfaces",
        "@features": "./src/features"
      }
    }]
  ]
}
